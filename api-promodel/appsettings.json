{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "promodeldrivers": {
    "couchdb": {
      "endpoint": "http://localhost:5984",
      "username": "admin",
      "password": "Pa$$w0rd"
    }
  },
  "Jwt": {
    "Issuer": "Issuer",
    "Audience": "Audience",
    "Key": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp}|{Level:u3}] {Message:lj} {Exception}{Properties:j}{NewLine}"
        }
      }
    ],
    "Enrich": [ "WithThreadId" ]
  },
  "AllowedHosts": "*"
}
